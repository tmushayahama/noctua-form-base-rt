    "build:staging": "rm -rf dist && tsc && vite build --mode staging",
    "build:production": "rm -rf dist && tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest run",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit"
